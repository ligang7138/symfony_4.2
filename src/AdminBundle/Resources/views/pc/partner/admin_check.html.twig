                                                </div>
                                                <div class="file-name">
                                                    上传证件资料
                                                </div>
                                            </div>
                                        </div>
                                        <div class="file-box" style="width: 140px !important;">
                                            <div class="file">
                                                <a href="javascript:void(0);" onclick="mopen('/upload/upload.html?f_type=a','资料图片上传',null,['750px','auto']);">
                                                    <span class="corner"></span>
                                                    <div class="image">
                                                        <img alt="image" class="img-responsive center-block" width="100%" src="/public/img/upload.png">
                                                    </div>
                                                    <div class="file-name">
                                                        点击上传图片
                                                    </div>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="panel-heading">
                                    <h3 class="panel-title">其他资料</h3>
                                </div>
                                <div class="form-group file_list clearfix">
                                    <div class="gallerys col-sm-offset-1 col-sm-8 filelist_{{ partner.partnerId | default(0) }}_g">
                                        <div class="file-box">
                                            <span class="cancel" data-id="{{ d.id }}" data-type="{{ d.pd_type }}">删除</span>
                                            <div class="file">
                                                <span class="corner"></span>
                                                <div class="image imgWrap">
                                                    <img alt="image" data-id="{{ d.ud_id }}" degree="{{ d.ud_degree }}" class="gallery-pic active rotate{{ d.ud_degree }} img-responsive center-block" src="{{ admin_bundle.fileHost }}" onclick="openPhotoWin(this)">
                                                </div>
                                                <div class="file-name">
                                                    上传其他资料
                                                </div>
                                            </div>
                                        </div>
                                        <div class="file-box" style="width: 140px !important;">
                                            <div class="file">
                                                <a href="javascript:void(0);" onclick="mopen('/upload/upload.html?f_type=g','资料图片上传',null,['750px','auto']);">
                                                    <span class="corner"></span>
                                                    <div class="image">
                                                        <img alt="image" class="img-responsive center-block" width="100%" src="/public/img/upload.png">
                                                    </div>
                                                    <div class="file-name">
                                                        点击上传图片
                                                    </div>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
							{% endif %}

							{#{% endfor %}#}
                        </div>
                    </div>
                </div>
                <div id="tabb-4" class="tab-pane">
                    <div class="panel-body">
                        <fieldset>
                            <div class="panel">
                                <div class="panel-heading">
                                    <h3 class="panel-title">商家审核</h3>
                                </div>
                                <div class="panel-body">
                                    <form class="form-horizontal fv-form fv-form-bootstrap" action="{{ path('admin_partner_checkadmin', {'id': partner.partnerId | default(0) }) }}" method="post" id="constraintsForm" onsubmit="return false;">
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label"><span class="must">*</span>审核意见：</label>
                                            <div class="col-md-6 col-sm-8">
                                                <div class="col-sm-12 col-md-3">
                                                    <div class="radio-custom radio-primary">
                                                        <input id="inlineRadio1" value="2" name="check_status"  checked type="radio">
                                                        <label for="inlineRadio1">通过</label>
                                                    </div>
                                                </div>
                                                <div class="col-sm-12 col-md-3">
                                                    <div class="radio-custom radio-primary">
                                                        <input id="inlineRadio2" value="1" name="check_status" type="radio">
                                                        <label for="inlineRadio2">拒绝</label>
                                                    </div>
                                                </div>
                                                <div class="col-sm-12 col-md-6">
                                                    <div class="radio-custom radio-primary">
                                                        <input id="inlineRadio3" value="3" name="check_status" type="radio">
                                                        <label for="inlineRadio3">打回（补充资料）</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group business_type">
                                            <label class="col-sm-3 control-label"><span class="must">*</span>商家类型：</label>
                                            <div class="col-md-6 col-sm-8">
                                                <div class="col-sm-15 col-md-3">
                                                    <div class="example-wrap">
                                                        <div class="radio-custom radio-primary">
                                                            <input type="radio"  {% if partner.partnerType == 1 %} checked="checked" {% endif %} name="partner_type" value="1">
                                                            <label for="inputChecked">普通商家</label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-15 col-md-3">
                                                    <div class="example-wrap">
                                                        <div class="radio-custom radio-primary">
                                                            <input type="radio"  {% if partner.partnerType == 2 %} checked="checked" {% endif %} name="partner_type" value="2">
                                                            <label for="inputUnchecked">信用商家</label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-15 col-md-3">
                                                    <div class="example-wrap">
                                                        <div class="radio-custom radio-primary">
                                                            <input type="radio"  {% if partner.partnerType == 3 %} checked="checked" {% endif %} name="partner_type" value="3">
                                                            <label for="inputChecked">自营商家</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group credit">
                                            <label class="col-sm-3 control-label"><span class="must">*</span>是否支持赊购：</label>
                                            <div class="col-md-6 col-sm-8">

                                                <div class="col-sm-15 col-md-3">
                                                    <div class="example-wrap">
                                                        <div class="radio-custom radio-primary">
                                                            <input type="radio"  {% if partner.isCreditBuy == 1 %} checked="checked" {% endif %} name="is_credit_buy" value="1">
                                                            <label for="inputChecked">是</label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-15 col-md-3">
                                                    <div class="example-wrap">
                                                        <div class="radio-custom radio-primary">
                                                            <input type="radio"  {% if partner.isCreditBuy == 2 %} checked="checked" {% endif %} name="is_credit_buy" value="2">
                                                            <label for="inputUnchecked">否</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group form-bolck">
                                            <label class="col-sm-3 control-label"><span class="must">*</span>审核备注：</label>
                                            <div class="col-md-8 col-sm-8">
                                                <textarea class="form-control" name="check_remark" rows="3" required="required" aria-required="true"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group form-bolck">
                                            <label class="col-sm-3 control-label"><span class="must">*</span>审核反馈：</label>
                                            <div class="col-sm-8">
                                                <textarea class="form-control" name="check_feedback" rows="3" required="required" aria-required="true"></textarea>
                                                <span>（“审核反馈”内容，将展示给商家查看）</span>
                                            </div>
                                        </div>
                                        <div class="col-sm-4 col-sm-offset-3" style="margin-top: 20px;">
                                            <input value="{{ partner.partnerId | default(0) }}" name="id" type="hidden">
                                            <button type="submit" class="btn btn-primary"  onclick="partner_submit();" id="validateButton">提交</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        // 居住地址初始化地址信息
		{% if adminUser.aProvince %}
        getCity("{{ adminUser.aProvince }}", "{{ adminUser.aCity }}");
		{% endif %}
		{% if adminUser.aCity %}
        getArea("{{ adminUser.aCity }}", "{{ adminUser.aCounty }}");
		{% endif %}

        //  初始化下拉框
        var config = {
            ".chosen-select": {},
            ".chosen-select-deselect": {
                allow_single_deselect: !0
            },
            ".chosen-select-no-single": {
                disable_search_threshold: 10
            },
            ".chosen-select-no-results": {
                no_results_text: "Oops, nothing found!"
            },
            ".chosen-select-width": {
                width: "100%"
            }
        };
        for (var selector in config) $(selector).chosen(config[selector]);
    });
    function getCity(v, select) {
        fajax("{{ path('admin_city_getcitylist') }}", 'c_id=' + v + '&select=' + select, 'post', 'html', function (data) {
            data = '<option value="">--请选择市--</option>' + data;
            $("#city").html(data);
            $("#city").chosen();
            $("#city").trigger("chosen:updated");
        })
    }
    function getArea(v, select) {
        fajax("{{ path('admin_city_getcitylist') }}", 'c_id=' + v + '&select=' + select, 'post', 'html', function (data) {
            data = '<option value="">--请选择县--</option>' + data;
            $("#county").html(data);
            $("#county").chosen();
            $("#county").trigger("chosen:updated");
        })
    }

    // 提交表单
    function partner_submit(){
        // confirm('您真的确定要提交进件信息吗？',function(){
        fsubmit($("#constraintsForm"),function(data){
            msg(data.msg,null,(300==data.code) ? 8 : (200==data.code ? 1 : 2));
            if(data.code == 200){
                close_tab();
                window.location.href = data.openUrl
            }else{
                alert(data.msg);
            }
        });
        // },'保存进件信息');
    }
   //  审核意见判断
   $("input:radio[name='check_status']").change(function() {
       if($("input:radio[name='check_status']:checked").val() != 2){
           $('.business_type').find('.must').hide();
           $('.credit').find('.must').hide();
       }else{
           $('.business_type').find('.must').show();
           $('.credit').find('.must').show();
       }
    });
   //   商家类型判断
   $("input:radio[name='partner_type']") .change(function () {
       if($("input:radio[name='partner_type']:checked").val() == 1){
           var fir = document.getElementsByName("is_credit_buy");
           for(var i=fir;i<fir.length;i++){
               fir[i].checked = false;
           }
           fir[1].checked = true;
           fir[0].disabled = true;
       }else {
           $("input:radio[name='is_credit_buy']").removeAttr("disabled","disabled");
       }
   })
</script>