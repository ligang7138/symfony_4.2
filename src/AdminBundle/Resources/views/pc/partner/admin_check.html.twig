<style>
    .form-bolck thead tr{
        background-color: #dff6fb;
    }
    .chosen-container{
        width: 100%!important;
    }
</style>
<div class="page-content" id="photo_box">
    <div class="panel nav-tabs-horizontal">
        <div class="panel-body" data-approve="nav-tabs">
            <div class="tabs-container">
                <ul class="nav nav-tabs">
                    <li class="active">
                        <a data-toggle="tab" href="#tabb-1" aria-expanded="true">实名认证信息</a>
                    </li>
                    <li class="">
                        <a data-toggle="tab" href="#tabb-2" aria-expanded="false">商家资料</a>
                    </li>
                    <li class="">
                        <a data-toggle="tab" href="#tabb-3" aria-expanded="false">附件信息</a>
                    </li>
                    <li class="">
                        <a data-toggle="tab" href="#tabb-4" aria-expanded="false">审核意见</a>
                    </li>
                </ul>
            </div>
            <div class="tab-content" id="xxlIntoPieceInfo">
                <div id="tabb-1" class="tab-pane active">
                    <div class="panel-body">
                        <div class="form-group">
                            <div class="panel-heading">
                                <h3 class="panel-title">个人认证信息</h3>
                            </div>
                            <div class="padding-20 padding-top-0 clearfix">
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">姓名:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aTrueName }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">身份证号:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aIdentNo }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">手机号:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aPhone }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">开户行:</label>

                                    <div class="col-sm-8 control-text">{{ adminUser.aOpenBankName }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">银行卡号:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aCardNo }}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="tabb-2" class="tab-pane">
                    <div class="panel-body">
                        <div class="form-group">
                            <div class="panel-heading">
                                <h3 class="panel-title">商家认证类型:{% if partner.partnerType == 2 %} 信用商家（提供金融服务） {% elseif partner.partnerType == 3 %}自营商家{% else %} 普通商家 {% endif %}</h3>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="panel-heading">
                                <h3 class="panel-title">个人基本信息</h3>
                            </div>
                            <div class="padding-20 padding-top-0 clearfix">
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">姓名:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aTrueName }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">身份证号:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aIdentNo }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">性别:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aIdentNo | getSexByIdCard}}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">出生日期:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aIdentNo | getBirthDayByIdCard}}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">年龄:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aIdentNo | getAgeByIdCard}}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">婚姻状况:</label>
                                    <div class="col-sm-8 control-text">

										{{ adminUser.aMaritalStatus | getValueByKey('AdminUser','MaritalStatus') }}

                                    </div>
                                </div>
                                <div class="form-bolck col-sm-12 padding-bottom-20">
                                    <label class="col-sm-2 control-label text-right">居住地址:</label>
                                    <div class="col-md-2 col-sm-3">
                                        <select name="provinces" id="provinces" class="chosen-select" data-rule-required="true" disabled>
                                            <option value="">--请选择省--</option>
											{% for pro in province_lists %}
                                                <option value="{{ pro.city_key }}"
												        {% if adminUser.aProvince == pro.city_key %}selected{% endif %}>{{ pro.city_name }}</option>
											{% endfor %}
                                        </select>
                                    </div>
                                    <div class="col-md-2 col-sm-3">
                                        <select name="city" disabled id="city" class="chosen-select" data-rule-required="true">
                                            <option value="">--请选择市--</option>
                                        </select>
                                    </div>
                                    <div class="col-md-2 col-sm-3">
                                        <select name="area" disabled id="county" class="chosen-select" data-rule-required="true">
                                            <option value="">--请选择县/区--</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">详细地址:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aDetailAddress }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">住宅电话:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aHomePhone }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">电子邮箱:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aiEmail }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">教育程度:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.aDegree | getValueByKey('AdminUser','EducateStatus') }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">共同居住人:</label>
                                    <div class="col-sm-8 control-text">{{ adminUser.togetherLivePerson }}</div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="panel-heading">
                                <h3 class="panel-title">商品信息</h3>
                            </div>
                            <div class="padding-20 padding-top-0 clearfix">
                                <div class="form-bolck col-sm-12 padding-bottom-20">
                                    <label class="col-sm-2 control-label text-right">销售品类:</label>
                                    <div class="col-sm-10 control-text">
										{% for cate in salesCategory %}

											{% if cate.gcId | inarray(partner.partnerIntention) %} {{ cate.gcName }}, {% endif %}
										{% endfor %}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="panel-heading">
                                <h3 class="panel-title">联系人信息</h3>
                            </div>
                            <div class="padding-20 padding-top-0">
                                <div class="form-bolck">
                                    <table class="table table-bordered table-hover dataTable table-striped width-full" id="contacts_list" >
                                        <thead>
                                        <tr>
                                            <th>姓名</th>
                                            <th>手机号</th>
                                            <th>身份证号</th>
                                            <th>性别</th>
                                            <th>居住地址</th>
                                            <th>职业</th>
                                            <th>年龄</th>
                                        </tr>
                                        </thead>
                                        <tbody>
										{% for key,adminContact in adminContacts %}
                                            <tr>
                                                <td>{{ adminContact.contactName }}</td>
                                                <td>{{ adminContact.phone }}</td>
                                                <td>{{ adminContact.identNo }}</td>
                                                <td>{{ adminContact.sex }}</td>
                                                <td>{{ adminContact.liveAddress }}</td>
                                                <td>{{ adminContact.profession }}</td>
                                                <td>{{ adminContact.age }}</td>
                                            </tr>
										{% endfor %}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="panel-heading">
                                <h3 class="panel-title">经营信息</h3>
                            </div>
                            <div class="padding-20 padding-top-0 clearfix">
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">经营地址:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.manageInfo | getValueByJson('business_address') }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">详细地址:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.manageInfo | getValueByJson('business_detail_address') }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">过往平均赊销占比:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.manageInfo | getValueByJson('past_average_credit_sales') }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">年销售额:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.manageInfo | getValueByJson('annual_sales') }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">毛利率:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.manageInfo | getValueByJson('gross_interest_rate') }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">净利率:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.manageInfo | getValueByJson('net_interest_rate') }}</div>
                                </div>
                                <div class="form-bolck">
                                    <table class="table table-bordered" style="margin: 0">
                                        <thead>
                                        <tr>
                                            <th height="50">其他经营信息</th>
                                        </tr>
                                        </thead>
                                        <tbody><tr>
                                            <td height="50">{{ partnerInfo.manageInfo | getValueByJson('other_manage_info') }}</td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="panel-heading">
                                <h3 class="panel-title">资产信息</h3>
                            </div>
                            <div class="padding-20 padding-top-0 clearfix">
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">应收款:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.assetInfo | getValueByJson('receivables') }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">存货(种类及数量):</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.assetInfo | getValueByJson('stock') }}</div>
                                </div>
                                <div class="form-bolck">
                                    <table class="table table-bordered" style="margin: 0">
                                        <thead>
                                        <tr>
                                            <th height="50">车辆信息</th>
                                        </tr>
                                        </thead>
                                        <tbody><tr>
                                            <td height="50">{{  partnerInfo.assetInfo | getValueByJson('vehicle_information') }}</td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-bolck">
                                    <table class="table table-bordered" style="margin: 0">
                                        <thead>
                                        <tr>
                                            <th height="50">住宅信息</th>
                                        </tr>
                                        </thead>
                                        <tbody><tr>
                                            <td height="50">{{  partnerInfo.assetInfo | getValueByJson('house_information') }}</td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-bolck">
                                    <table class="table table-bordered" style="margin: 0">
                                        <thead>
                                        <tr>
                                            <th height="50">门店信息</th>
                                        </tr>
                                        </thead>
                                        <tbody><tr>
                                            <td height="50">{{  partnerInfo.assetInfo | getValueByJson('store_information') }}</td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class="form-bolck">
                                    <table class="table table-bordered" style="margin: 0">
                                        <thead>
                                        <tr>
                                            <th height="50">其他资产信息</th>
                                        </tr>
                                        </thead>
                                        <tbody><tr>
                                            <td height="50">{{  partnerInfo.assetInfo | getValueByJson('other_asset_information') }}</td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="panel-heading">
                                <h3 class="panel-title">负债信息</h3>
                            </div>
                            <div class="padding-20 padding-top-0 clearfix">
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">银行贷款:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.debtInfo | getValueByJson('bank_loan') }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">小贷公司借款:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.debtInfo | getValueByJson('company_loan') }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">私人借款:</label>
                                    <div class="col-sm-8 control-text">{{  partnerInfo.debtInfo | getValueByJson('private_loan') }}</div>
                                </div>
                                <div class="form-bolck">
                                    <table class="table table-bordered" style="margin: 0">
                                        <thead>
                                        <tr>
                                            <th height="50">其他负债信息</th>
                                        </tr>
                                        </thead>
                                        <tbody><tr>
                                            <td height="50">{{  partnerInfo.debtInfo | getValueByJson('other_debt_loan') }}</td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="panel-heading">
                                <h3 class="panel-title">受理信息</h3>
                            </div>
                            <div class="padding-20 padding-top-0 clearfix">
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">受理营业部:</label>
                                    <div class="col-sm-8 control-text">
										{% for department in departments %}
											{% if department.dtId == partnerInfo.acceptDepartment %}{{ department.dtName }}{% endif %}
										{% endfor %}
                                    </div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">营业部经理:</label>
                                    <div class="col-sm-8 control-text">{{ partnerInfo.saleManager }}</div>
                                </div>
                                <div class="form-bolck col-sm-6 padding-bottom-20">
                                    <label class="col-sm-4 control-label text-right">办理人员:</label>
                                    <div class="col-sm-8 control-text">{{ partnerInfo.acceptOfficer }}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
