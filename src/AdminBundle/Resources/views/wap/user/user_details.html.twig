<style>
	.control-text {
		padding-top: 7px;
		margin-bottom: 0;
		text-align: left;
	}

	.file-box {
		width: 208px !important;
	}
</style>
<div class="wrapper wrapper-content fadeInRight" id="photo_box">
	<div class="row">
		<div class="col-sm-12">
			<div class="ibox ">
				<div class="ibox-content">
					<form class="form-horizontal m-t" id="commentForm"
					      action="{{ path('admin_user_showuser',{'uCode':userInfo.u_code}) }}" method="post"
					      onsubmit="fsubmit(this);return false;">
						<fieldset>
							<legend>借款人信息</legend>
							<div class="form-group">
								<div class="form-bolck">
									<label class="col-sm-2 control-label">客户编号：</label>
									<div class="col-sm-3 control-text">
										<span>{{ userInfo.u_code }}</span>
									</div>
								</div>
								<div class="form-bolck">
									<label class="col-sm-2 control-label">客户类型：</label>
									<div class="col-sm-3 control-text">
										<span>{{ userInfo.u_type == 0 ? '个人' : '企业' }}</span>
									</div>
								</div>

							</div>
							<div class="form-group">
								<div class="form-bolck">
									<label class="col-sm-2 control-label">客户名称：</label>
									<div class="col-sm-3 control-text">
										<span>{{ userInfo.ui_true_name }}</span>
									</div>
								</div>
								<div class="form-bolck">
									<label class="col-sm-2 control-label">证件编号：</label>
									<div class="col-sm-3 control-text">
										<span>{{ userInfo.ui_ident_no }}</span>
									</div>
								</div>
							</div>
							<div class="form-group">
								<div class="form-bolck">
									<label class="col-sm-2 control-label">手机号：</label>
									<div class="col-sm-3 control-text">
										<span>{{ userInfo.ur_phone }}</span>
									</div>
								</div>
								<div class="form-bolck">
									<label class="col-sm-2 control-label">注册时间：</label>
									<div class="col-sm-3 control-text">
										<span>{{ userInfo.u_reg_time }}</span>
									</div>
								</div>
							</div>
							<div class="form-group">
								<div class="form-bolck">
									<label class="col-sm-2 control-label">服务编号：</label>
									<div class="col-sm-3 control-text">
										<span>{{ userInfo.u_service_code}}</span>
									</div>
								</div>
								<div class="form-bolck">
									<label class="col-sm-2 control-label">状态：</label>
									<div class="col-sm-3 control-text">
										<span>{{ userInfo.u_type == 1 ? '启用' : '停用' }}</span>
									</div>
								</div>
							</div>
						</fieldset>
					</form>
				</div>
			</div>
		</div>
	</div>
</div>