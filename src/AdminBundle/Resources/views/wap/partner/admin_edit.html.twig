<script type="text/javascript">data_str='{{ "now"|date("Y-m-d") }}';</script>
<script>
    $(document).ready(function () {
        // 居住地址初始化地址信息
		{% if adminUser.aProvince %}
        getCity("{{ adminUser.aProvince }}", "{{ adminUser.aCity }}");
		{% endif %}
		{% if adminUser.aCity %}
        getArea("{{ adminUser.aCity }}", "{{ adminUser.aCounty }}");
		{% endif %}

        //  初始化下拉框
        var config = {
            ".chosen-select": {},
            ".chosen-select-deselect": {
                allow_single_deselect: !0
            },
            ".chosen-select-no-single": {
                disable_search_threshold: 10
            },
            ".chosen-select-no-results": {
                no_results_text: "Oops, nothing found!"
            },
            ".chosen-select-width": {
                width: "100%"
            }
        };
        for (var selector in config) $(selector).chosen(config[selector]);
    });
    // input框失去焦点判断
    $("input[required]").parent().append('<span id="uei_operate_time-error" class="help-block m-b-none" style="display: none"><i class="fa fa-times-circle"></i>  必填</span>')
    $('input[required]').blur(function(){
        if($(this).val()==''){
            $(this).parents('.form-bolck').addClass('has-error');
            $(this).parent().find('#uei_operate_time-error').css('display','inline-block');
            $('.btn_next').attr({"disabled":"disabled"});
            return false;
        }else{
            $(this).parents('.form-bolck').removeClass('has-error').addClass('has-success');
            $(this).parent().find('#uei_operate_time-error').css('display','none');
            $('.btn_next').removeAttr("disabled");
            return true;
        }
    });
    function getCity(v, select) {
        fajax("{{ path('admin_city_getcitylist') }}", 'c_id=' + v + '&select=' + select, 'post', 'html', function (data) {
            data = '<option value="">--请选择市--</option>' + data;
            $("#city").html(data);
            $("#city").chosen();
            $("#city").trigger("chosen:updated");
        })
    }
    function getArea(v, select) {
        fajax("{{ path('admin_city_getcitylist') }}", 'c_id=' + v + '&select=' + select, 'post', 'html', function (data) {
            data = '<option value="">--请选择县--</option>' + data;
            $("#county").html(data);
            $("#county").chosen();
            $("#county").trigger("chosen:updated");
        })
    }
    //  点击添加联系人
    $('#contacts_add').on('click', function () {
        var contacts_rowTem = '<tr>'
            + '<td><input name="contact_name[]" type="text" class="input_hidden"></td>'
            + '<td><input name="contact_phone[]" type="text" class="input_hidden"></td>'
            + '<td><input name="contact_card[]" type="text" class="input_hidden" onblur="checkCard($(this),\'.contact_sex\',\'.contact_age\')"></td>'
            + '<td><input name="contact_sex[]" type="text" class="input_hidden contact_sex"></td>'
            + '<td><input name="contact_address[]" type="text" class="input_hidden"></td>'
            + '<td><input name="contact_profession[]" type="text" class="input_hidden"></td>'
            + '<td><input name="contact_age[]" type="text" class="input_hidden contact_age"></td>'
            + '<input name="contact_id[]" type="hidden" class="input_hidden">'
            + '<td> '
            + '    <a class="del_btn">删除</a>'
            + '</td>'
            + '</tr>';
        $("#contacts_list tbody:last").append(contacts_rowTem);
    });
    //    点击删除tr
    $('.table_add').delegate('.del_btn', 'click', function(){
        $(this).parents('tr').remove();
    });
    //   点击下一步按钮页面回到最顶部
    $('.btn_next').click(function () {
        var index = $(this).parents('.wizard-pane').index();
        $(this).parents('.wizard-pane').next().addClass('active').siblings().removeClass('active');
        $('.pearls .pearl').eq(index).addClass('done').removeClass('current').next().addClass('current').removeClass('disabled')
        $('body,html').animate({
            scrollTop: 0
        },0);
    });
    //  点击上一步
    $('.btn_back').click(function () {
        var index = $(this).parents('.wizard-pane').index();
        $(this).parents('.wizard-pane').prev().addClass('active').siblings().removeClass('active');
        $('.pearls .pearl').eq(index).addClass('disabled').removeClass('done current').prev().addClass('current done').removeClass('disabled')
    });
    // 提交表单
    function partner_submit(){
        // confirm('您真的确定要提交进件信息吗？',function(){
        fsubmit($("#submitbaseinfo"),function(data){
            msg(data.msg,null,(300==data.code) ? 8 : (200==data.code ? 1 : 2));
            if(data.code == 200){
                window.location.href = data.openUrl;
            }else{
                alert(data.msg);
            }
        });
        // },'保存进件信息');
    }
    // 删除资料
    $(".file_list").delegate(".cancel","click",function(){
        obj = new Object();
        obj.id = $(this).attr('data-id');
        obj.p_type = $(this).attr('data-type');
        var that = $(this);
        fajax('/upload/del.html',obj,'post','json',function(d){
            if(200 == d.code){
                that.parent().remove();
            }else if(201 == d.code){
                that.parent().remove();
            }
            alert(d.msg);
        });
    });

    // 删除资料
    $(".file-box").delegate(".cancelGcate","click",function(){
        obj = new Object();
        obj.id = $(this).attr('data-id');
        var that = $(this);
        fajax('/gcate/del_img.html',obj,'post','json',function(d){
            alert(d.msg);
            if(200 == d.code){
                that.parent().remove();
                $.pjax({url: d.openUrl, container: '#admui-pageContent'});
            }else if(201 == d.code){
                that.parent().remove();
            }
        });
    });
    // 暂存商户信息
    function partnerCacheInfo(){
        var form = $('#submitbaseinfo');
        var pdata = form.serialize();
        fajax(form.attr('action')+'?storage=1',pdata,'post','json',function (data) {
            if(data.code == 200) {
                // console.log(data);return false;
                window.location.href = data.openUrl;
            }else{
                alert(data.msg);
            }
        })
    }

</script>