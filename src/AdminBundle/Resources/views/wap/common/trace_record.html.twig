<div class="wrapper wrapper-content fadeInRight">
    <div class="row">
        <div class="col-sm-14">
            <div class="ibox ">
               
                <div class="ibox-content">
                    <div class="jqGrid_wrapper">
                        <div class="ui-jqgrid " id="gbox_table_list_1" dir="ltr" >
                            <div class="ui-jqgrid-view table-responsive" role="grid" id="gview_table_list_1">

                                <div  class="ui-jqgrid-hdiv">
                                    <div class="ui-jqgrid-hbox">
                                        <table rel="table" class="ui-jqgrid-htable ui-common-table table table-bordered" role="presentation" aria-labelledby="gbox_table_list_1">
                                            <thead>
                                            <tr class="ui-jqgrid-labels" role="row">
                                                <th style="width:15%;">操作人 </th>
                                                <th style="width:20%;">操作时间</th>
                                                {% if type in 0..1 and type !=2 %}<th style="width:20%;">操作前状态</th>{% endif %}
                                                {% if type == 0 and type !=2 %}<th style="width:20%;">当前状态</th>{% endif %}
                                                <th>审核备注说明</th>
                                            </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                                <div class="ui-jqgrid-bdiv">
                                    <div style="position:relative;">
                                        <div>
                                        </div>
                                        <table class="table table-bordered banklist">
                                            <tbody>
                                            {% if behaviorTraceList != null %}
                                                {% for key,behaviorTrace in behaviorTraceList %}
                                                    <tr class="{% if loop.first %} success {% endif %}">
                                                        <td style="width:15%;">{{ behaviorTrace.bt_op_name }}</td>
                                                        <td style="width:20%;">{{ behaviorTrace.bt_op_time|date('Y-m-d H:i:s') }}</td>
                                                        {% if type in 0..1 and type !=2 %}<td style="width:20%;">
                                                        {% if behaviorTrace.bt_before_status is same as(0) %}{{ admin_bundle.status["0000"]}}{% else %}{{ admin_bundle.status[behaviorTrace.bt_before_status]}}
                                                               {% endif %}
                                                        </td>{% endif %}
                                                        {% if type == 0 and type !=2 %}<td style="width:20%;"><font color="blue"><b>
                                                           {% if behaviorTrace.bt_status is same as(0) %}{{ admin_bundle.status["0000"]}}{% else %}{{ admin_bundle.status[behaviorTrace.bt_status]}}
                                                               {% endif %} </b></font></td>{% endif %}
                                                        <td>{{ behaviorTrace.bt_remark }}</td>
                                                    </tr>
                                                {% endfor %}
                                            {% else %}
                                                <tr>
                                                    <td align="center" colspan="20"><font color="red">暂无数据!</font></td>
                                                </tr>
                                            {% endif %}
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <div class="ui-jqgrid-resize-mark" id="rs_mtable_list_1">
                                &nbsp;
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>