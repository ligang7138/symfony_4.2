{% block css_common %}
	<link href="{{ '/public/css/plugins/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css'|gfut }}"
	      rel="stylesheet">
	<link href="{{ '/public/css/plugins/iCheck/custom.css'|gfut }}" rel="stylesheet">
	<link href="{{ '/public/css/plugins/chosen/chosen.css'|gfut }}" rel="stylesheet">
	<link href="{{ '/public/css/plugins/x-editable/bootstrap-editable.css'|gfut }}" rel="stylesheet">
	<link href={{ "/public/js/plugins/photo_allery/photoGallery.css"|gfut }} rel="stylesheet">
	<style>
		.lt-bar {
			font-size: 16px;
			border-left: 6px solid #1AB394;
			padding-left: 10px;
		}
		.one-content {
			padding: 15px 20px;
		}

		.one-content .active {
			width: 50%;
		}

		.list {
			margin-bottom: 75px;
		}

		.left-bar {
			border-left: 6px solid #1AB394;
			min-height: 25px;
			padding-left: 10px;
			margin: 20px 0 25px;
			line-height: inherit;
		}

		.left-bar .left-bar-title {
			padding-right: 20px;
		}

		.ui-jqgrid tr.jqgfirstrow td {
			padding: 10px;
		}

		.data-money {
			display: inline-block;
			min-width: 10px;
		}

		/* 这里是必填项 */
		.must {
			color: red;
			font-size: 30px;
			padding: 0 5px;
			vertical-align: -webkit-baseline-middle;
		}
	</style>
{% endblock %}
{% block box %}
	<div class="gray-bg">
		<div class="wrapper wrapper-content fadeInRight" id="photo_box">
			<div class="row">
				<div class="col-sm-12">
					<div class="ibox">
						<div class="ibox-content">
							<div class="tabs-container">
								<ul class="nav nav-tabs">
									<li class="active">
										<a data-toggle="tab" href="#tabb-{{ tabArr.one }}" aria-expanded="true">借款信息</a>
									</li>
									<li class="">
										<a data-toggle="tab" href="#tabb-{{ tabArr.two }}"
										   aria-expanded="false">进件信息</a>
									</li>
									<li class="">
										<a data-toggle="tab" href="#tabb-{{ tabArr.three }}"
										   aria-expanded="false">附件信息</a>
									</li>
									<li class="">
										<a data-toggle="tab" href="#tabb-{{ tabArr.four }}"
										   aria-expanded="false">审核信息</a>
									</li>
									{% block tab %}
									{% endblock %}
									{% if apply_info.b_status > 2410 %}
										<li class="">
											<a data-toggle="tab" href="#tabb-{{ tabArr.signInfo }}"
											   aria-expanded="false">签约信息</a>
										</li>
									{% endif %}
								</ul>
							</div>
							{% block form %}
							<form class="form-horizontal m-t" enctype="multipart/form-data" id="up_image_form"
							      method="post">
								{% endblock %}
								<div class="tab-content" id="xlcommonIntoPiece">
									<div id="tabb-{{ tabArr.one }}" class="tab-pane active">
										<fieldset>
											<div class="col-sm-6 one-content">
												<table rel="table"
												       class="ui-jqgrid-htable ui-common-table table table-bordered"
												       role="presentation" aria-labelledby="gbox_table_list_1">
													<tr>
														<th class="active">客户编号</th>
														<th> {{ apply_info.u_code }}</th>
													</tr>
													<tr>
														<th class="active">推广人员</th>
														<th> {{ apply_info.b_service_no|valToStrByMap(leaders) }}</th>
													</tr>
													<tr>
														<th class="active">客户申请时间</th>
														<th> {{ apply_info.b_apply_time }}</th>
													</tr>
													<tr>
														<th class="active">项目名称</th>
														<th>{{ apply_info.pr_name }}</th>
													</tr>
													<tr>
														<th class="active">产品名称</th>
														<th>{{ apply_info.p_name }}</th>
													</tr>
												</table>
											</div>
											<div class="col-sm-6 one-content">
												<table rel="table"
												       class="ui-jqgrid-htable ui-common-table table table-bordered"
												       role="presentation" aria-labelledby="gbox_table_list_1">
													<tr>
														<th class="active">还款方式</th>
														<th>
                                                            <span>
                                                                {{ attribute(admin_bundle.returnType,apply_info.p_return_type) }}
                                                           </span>
														</th>
													</tr>
													<tr>
														<th class="active">产品期限</th>
														<th>{{ apply_info.p_period }}{% if pKind == 2 %}天{% else %}月{% endif %}</th>
													</tr>
													<tr>
														<th class="active">借款用途</th>
														<th>{{ apply_info.b_borrow_use_remark }}</th>
													</tr>
													<tr>
														<th class="active">客户来源</th>
														<th>
																	<span>
															          {% if apply_info.u_source == 1 %}老客户续贷
															          {% elseif apply_info.u_source == 2 %}公司资源
															          {% elseif apply_info.u_source == 3 %}自主开发
															          {% elseif apply_info.u_source == 4 %}转介绍
															          {% else %}其他
															          {% endif %}
                                                                   </span>
														</th>
													</tr>
													<tr>
														<th class="active">申请金额(元)</th>
														<th style="position: relative">
															{{ apply_info.b_borrow_amount }}
															{% if apply_info.p_return_type != 2 %}
																<button type="button" class="btn btn-info btn-xs"
																        style="position: absolute;right: 0;top: 1px;"
																        onclick="fajax('/admin/get_return_list/{{ apply_info.p_id|default(0) }}.html','amount={% if apply_info.b_status <= 2100 %}{{ apply_info.b_borrow_amount }}{% elseif apply_info.b_status > 2100 and apply_info.b_status <= 2200 %}{{ apply_info.c2_proposal_amt }}{% elseif apply_info.b_status > 2200 and apply_info.b_status < 2500 %}{{ apply_info.c3_proposal_amt }}{% else %}{{ apply_info.c_amount }}{% endif %}&loan_start_date={{ apply_info.b_apply_time }}','post','json',function(data){show_return_list(data);});">
																	还款试算
																</button>
															{% endif %}
														</th>
													</tr>
												</table>
											</div>
										</fieldset>
									</div>
									<div id="tabb-{{ tabArr.two }}" class="tab-pane">
										<div class="panel-body">
											<fieldset>
												<div class="list">
													<h4 class="example-title left-bar">
														<span class="left-bar-title">借款人基本信息</span>
														<span class="ibox-tools">加<big class="must">*</big>号的为必填项</span>
													</h4>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>客户类型：</label>
															<div class="col-sm-3 control-text">{% if apply_info.u_type==1 %}企业{% else %}个人{% endif %}</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>客户名称：</label>
															<div class="col-sm-3 control-text">{{ apply_info.ui_true_name }}</div>
														</div>
													</div>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>手机号：</label>
															<div class="col-sm-3 control-text">{{ apply_info.ui_phone }}</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>身份证号：</label>
															<div class="col-sm-3 control-text">{{ apply_info.ui_ident_no }}</div>
														</div>
													</div>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>年龄：</label>
															<div class="col-sm-3 control-text">{{ apply_info.age }}</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>居住年限：</label>
															<div class="col-sm-3 control-text">{{ userCurrInfo.ur_live_year }}
																年
															</div>
														</div>
													</div>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>教育程度：</label>
															<div class="col-sm-3 control-text">
                                                         <span>
															{% if apply_info.ui_edu_degree == 0 %}初中及以下
															{% elseif apply_info.ui_edu_degree == 1 %}高中
															{% elseif apply_info.ui_edu_degree == 2 %}大专
															{% elseif apply_info.ui_edu_degree == 3 %}本科及以上
															{% endif %}
                                                         </span>
															</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>居住状况：</label>
															<div class="col-sm-3 control-text">
                                                         <span>
															{% if apply_info.ui_house_type == 1 %}商品房
															{% elseif apply_info.ui_house_type == 2 %}自建房
															{% elseif apply_info.ui_house_type == 3 %}宅基地
															{% elseif apply_info.ui_house_type == 4 %}租赁
															{% endif %}
                                                         </span>
															</div>
														</div>
													</div>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>共同居住人：</label>
															<div class="col-sm-3 control-text">
                                                           <span>
															  {% if apply_info.ui_together_live_person.po == 1 %}配偶
															  {% elseif apply_info.ui_together_live_person.fm == 2 %}父母
															  {% elseif apply_info.ui_together_live_person.zn == 3 %}子女
															  {% elseif apply_info.ui_together_live_person.qt == 4 %}其他
															  {% endif %}
                                                           </span>
															</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>职业：</label>
															<div class="col-sm-3 control-text">{{ userCurrInfo.ur_work_info }}</div>
														</div>
													</div>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label">子女数量：</label>
															<div class="col-sm-3 control-text">{{ apply_info.ui_son_daughter_num }}</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label">住宅电话：</label>
															<div class="col-sm-3 control-text">{{ apply_info.ui_fix_phone }}</div>
														</div>
													</div>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label">微信/QQ：</label>
															<div class="col-sm-3 control-text">{{ apply_info.ui_qq_wechat }}</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label"><span
																		class="must">*</span>居住地址：</label>
															<div class="col-sm-3 control-text">
                                                           <span>
                                                            {{ userCurrInfo.live_province }}
	                                                           {{ userCurrInfo.live_city }}
	                                                           {{ userCurrInfo.live_area }}
	                                                           {{ userCurrInfo.ur_live_address }}
                                                           </span>
															</div>
														</div>
													</div>
												</div>
												<div class="list">
													<h4 class="example-title left-bar">
														<span class="left-bar-title">联系人基本信息</span>
													</h4>
													<table class="table table-bordered">
														<thead>
														<tr>
															<th> 姓名</th>
															<th> 联系电话</th>
															<th> 地址</th>
															<th> 与借款人关系</th>
															<th> 身份证</th>
															<th> 性别</th>
															<th> 职业/工作</th>
															<th> 居住年限</th>
															<th> 年龄</th>
														</tr>
														</thead>
														<tbody>
														{% for r in userRelation %}
															<tr class="jqgfirstrow" role="row">
																<td role="gridcell">{{ r.ur_name }}</td>
																<td role="gridcell">{{ r.ur_phone }}</td>
																<td role="gridcell">{{ r.ur_live_address }}</td>
																<td role="gridcell">{{ attribute(admin_bundle.relation_status,r.ur_relation) }}</td>
																<td role="gridcell">{{ r.ur_ident_no }}</td>
                                                                    <td role="gridcell">{% if r.ur_sex==1 %}男{% else %}女{% endif %}</td>
																<td role="gridcell">{{ r.ur_work_info }}</td>
																<td role="gridcell">{{ r.ur_live_year }}</td>
																<td role="gridcell">{{ r.ur_age }}</td>
															</tr>
														{% else %}
															<tr class="warning">
																<td colspan="10"><em class="text-info">暂无数据！</em>
																</td>
															</tr>
														{% endfor %}
														</tbody>
													</table>
												</div>
												<div class="list">
													<h4 class="example-title left-bar">
														<span class="left-bar-title">共同借款人</span>
													</h4>
													<table class="table table-bordered">
														<thead>
														<tr>
															<th>姓名</th>
															<th>身份证号</th>
															<th>手机</th>
															<th>共同借款人关系</th>
															<th>开户行</th>
															<th>银行卡号</th>
														</tr>
														</thead>
														<tbody>
														{% for co in coBorrowInfo %}
															<tr>
																<td>{{ co.cb_true_name }}</td>
																<td>{{ co.cb_ident_no }}</td>
																<td>{{ co.cb_phone }}</td>
																<td>{{ attribute(admin_bundle.coborrower_type,co.cb_type) }}</td>
																<td>{{ co.cb_open_bank }}</td>
																<td>{{ co.cb_bank_card_no }}</td>
															</tr>
														{% else %}
															<tr class="warning">
																<td colspan="10"><em class="text-info">暂无数据！</em>
																</td>
															</tr>
														{% endfor %}
														</tbody>
													</table>
												</div>
												<div class="list">
													<h4 class="example-title left-bar">
														<span class="left-bar-title">资产状况</span>
													</h4>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label">现金：</label>
															<div class="col-sm-3 control-text">{{ userFundInfo.u_cash_deposit_info.fund_info_cash }}
																元
															</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label">银行存款：</label>
															<div class="col-sm-3 control-text">{{ userFundInfo.u_cash_deposit_info.fund_info_deposit }}
																元
															</div>
														</div>
													</div>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label">应收账款(生意)：</label>
															<div class="col-sm-3 control-text">{{ userFundInfo.u_receivable_account.receivable_business }}
																元
															</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label">应收账款(个人)：</label>
															<div class="col-sm-3 control-text">{{ userFundInfo.u_receivable_account.receivable_person }}
																元
															</div>
														</div>
													</div>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label">预付房租：</label>
															<div class="col-sm-3 control-text">{{ userFundInfo.u_prepay_info.prepay_rent }}
																元
															</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label">预付货款：</label>
															<div class="col-sm-3 control-text">{{ userFundInfo.u_prepay_info.prepay_goods_payment }}
																元
															</div>
														</div>
													</div>
													<table class="table table-bordered">
														<thead>
														<tr>
															<th colspan="10" height="50px">资产状况描述</th>
														</tr>
														</thead>
														<tr>
															<td colspan="10"
															    height="50px">{{ userFundInfo.u_examine_opinion }}</td>
														</tr>
													</table>
												</div>
												<div class="list">
													<h4 class="example-title left-bar">
														<span class="left-bar-title">存货</span>
													</h4>
													<table class="table table-bordered" style="margin: 0">
														<tbody>
														<tr bgcolor="f5f5f3">
															<th>种类</th>
															<th>数量</th>
															<th>单价</th>
															<th>总额</th>
														</tr>
														{% for key,goods in userFundInfo.u_existing_goods %}
															<tr>
																<td><span class="data-money">{{ goods.kind }}</span>
																</td>
																<td><span class="data-money">{{ goods.num }}</span>个
																</td>
																<td>
																	<span class="data-money">{{ goods.price }}</span>元
																</td>
																<td>
																	<span class="data-money">{{ goods.amount }}</span>元
																</td>
															</tr>
														{% else %}
															<tr class="warning">
																<td colspan="10"><em class="text-info">暂无数据！</em>
																</td>
															</tr>
														{% endfor %}
														</tbody>
													</table>
												</div>
												<div class="list">
													<h4 class="example-title left-bar">
														<span class="left-bar-title">固定资产</span>
													</h4>
													<table class="table table-bordered" style="margin: 0">
														<tbody>
														<tr bgcolor="f5f5f3">
															<th>种类</th>
															<th>数量</th>
															<th>单价</th>
															<th>总额</th>
														</tr>
														{% for key,fixed in userFundInfo.u_fixed_assets %}
															<tr>
																<td><span class="data-money">{{ fixed.kind }}</span>
																</td>
																<td><span class="data-money">{{ fixed.num }}</span>个
																</td>
																<td>
																	<span class="data-money">{{ fixed.price }}</span>元
																</td>
																<td>
																	<span class="data-money">{{ fixed.amount }}</span>元
																</td>
															</tr>
														{% else %}
															<tr class="warning">
																<td colspan="10"><em class="text-info">暂无数据！</em>
																</td>
															</tr>
														{% endfor %}
														</tbody>
													</table>
												</div>
												<div class="list">
													<h4 class="example-title left-bar">
														<span class="left-bar-title">总资产</span>
													</h4>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label">总资产：</label>
															<div class="col-sm-3 control-text">{{ userFundInfo.u_total_amount | number_format(2,'.','') }}
																元
															</div>
														</div>
													</div>
												</div>
												<div class="list">
													<h4 class="example-title left-bar">
														<span class="left-bar-title">负债状况</span>
													</h4>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label">应付房租：</label>
															<div class="col-sm-3 control-text">{{ userLiabilitiesInfo.u_payable_money.payable_rent | number_format(2,'.','') }}
																元
															</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label">应付水电费：</label>
															<div class="col-sm-3 control-text">{{ userLiabilitiesInfo.u_payable_money.payable_water_ele | number_format(2,'.','') }}
																元
															</div>
														</div>
													</div>
													<div class="form-group">
														<div class="form-bolck">
															<label class="col-sm-2 control-label">应付工资：</label>
															<div class="col-sm-3 control-text">{{ userLiabilitiesInfo.u_payable_money.payable_wages | number_format(2,'.','') }}
																元</p>
															</div>
														</div>
														<div class="form-bolck">
															<label class="col-sm-2 control-label">应付货款：</label>
															<div class="col-sm-3 control-text">{{ userLiabilitiesInfo.u_payable_money.payable_goods_payment | number_format(2,'.','') }}
																元
															</div>
														</div>
