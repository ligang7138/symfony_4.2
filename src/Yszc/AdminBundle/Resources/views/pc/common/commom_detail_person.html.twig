	<script language="javascript">
        $(function () {
            $(".file_list").delegate(".cancel", "click", function () {
                obj = new Object();
                obj.ubid = $(this).attr('data-id');
                var that = $(this);
                fajax('/admin/upload/del.html', obj, 'post', 'json', function (d) {
                    if (200 == d.code) {
                        that.parent().remove();
                    }
                    alert(d.msg);
                });
            });


            $("#pdfView_{{ b_id }}").click(function () {
                var url = $(this).attr("src");
                if (url == '') {
                    alert('您还没有上传文件!');
                    return false;
                }
                window.open(url, '_blank');
            })
        })
        function getContract(type) {

            var bid = {{ b_id }};

            var url = '/contract/getpdf/' + bid + '-' + type + '.html';

            window.open(url, '_blank');

        }
        function image_upload(ud_id) {
            if ($("#fileToUpload" + ud_id).val().length == 0) {
                alert('请选择图片文件!');
                return false;
            } else if (!/.+\.jpg/.test($("#fileToUpload" + ud_id).val()) && !/.+\.png/.test($("#fileToUpload" + ud_id).val())) {
                alert('图片文件格式不正确，请选择jpg|png文件!');
                return false;
            } else {
                $('#up_image_form').ajaxSubmit({
                    url: "{{ admin_bundle.uploadFileUrl }}",
                    type: 'POST',
                    beforeSubmit: function (a, b, c) {
                        backOff(true);
                    },
                    headers: {'Authorization': hex_md5(data_str)},
                    success: function (d) {
                        msg(d.msg);
                        if (2000 == d.status) {
                            fajax('/admin/upload/edit_save.html', 'udid=' + ud_id + '&url=' + d.result.url, 'post', 'json', function (data) {
                                alert(data.msg);
                                $('#corner' + data.udid).removeClass('corner').addClass('select_corner');
                                $('#file' + data.udid).css('border', '1px solid red');
                            });
                        } else {
                            alert(d.msg);
                        }
                        backOff(false);
                    },
                    error: function (result) {
                        console.log(result);
                        msg('上传失败!');
                        backOff(false);
                    },
                    resetForm: true,
                    dataType: 'json'
                });
                return false;
            }
        }
	</script>
