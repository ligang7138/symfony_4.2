    var searchData=get{{ tabid }}SearchData();
    if(output){
        searchData.output = output;
        $('.J_iframe').each(function () {
            if (!$(this).is(":hidden")) {
               url  = $(this).data('id');
               var jdata=$.toJSON(searchData);
               //调用方法 如        
                post(url, {p :nums,jdata:jdata}); 
            }
        });
        

    }else{
